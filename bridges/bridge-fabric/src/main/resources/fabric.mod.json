{
  "schemaVersion": 1,
  "id": "fabric_bridge",
  "version": "${version}",
  "name": "FabricBridge",
  "description": "Enables proxy support for Fabric servers in PoloCloud.",
  "authors": [
    "RECHERGG"
  ],
  "contact": {
    "homepage": "https://polocloud.de",
    "sources": "https://github.com/HttpMarco/polocloud"
  },
  "license": "Apache 2.0",
  "environment": "server",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.httpmarco.polocloud.bridges.fabric.FabricBridge"
      }
    ]
  },
  "mixins": [
    "polocloud_bridge.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric-networking-api-v1": "*",
    "minecraft": ">=1.21",
    "java": ">=21",
    "fabric-language-kotlin": ">=1.13.4+kotlin.2.2.0"
  },
  "accessWidener": "polocloud_bridge.accesswidener"
}